<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:Lind.MAUI.Example.ViewModels;assembly=Lind.MAUI.Example.ViewModels"
             x:DataType="vm:NavigationViewModel"
             x:Class="Lind.MAUI.Example.NavigationView">
    <VerticalStackLayout>
        <Button
            Text="Navigate to MainPage"
            Command="{Binding MainWindowNav}"
            HorizontalOptions="Center" />
        <Button Text="Navigate to Counter"
                Command="{Binding CounterNav}"
                HorizontalOptions="Center" />
    </VerticalStackLayout>
</ContentView>
